}
.box {
  float: left;
  margin-right: 15px;
  width: calc(1.66666667%);
  color: #fff;
  padding: 20px 10px;
  text-align: center;
  border-radius: 5px;
}
.box p:first-child {
  font-size: 16px;
  margin-bottom: 5px;
}
.box p:nth-child(2) {
  font-size: 12px;
  margin-bottom: 0px;
}
